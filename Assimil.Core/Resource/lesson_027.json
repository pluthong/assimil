    "english_lesson": [
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Here are some idioms. We already know some of them.",
                    "frenchspeech": "",
                    "beginplay": 4,
                    "endplay": 10.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Please close the window, my wife is cold.",
                    "frenchspeech": "",
                    "beginplay": 10.5,
                    "endplay": 15.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "You are very lucky to have a charming wife.",
                    "frenchspeech": "",
                    "beginplay": 15.5,
                    "endplay": 21.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "I don't want to swim. I'm scared of fish, especially sharks. (1)",
                    "frenchspeech": "",
                    "beginplay": 21.5,
                    "endplay": 29.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "You are right, it's very hot outside but I'm not hot.",
                    "frenchspeech": "",
                    "beginplay": 29.5,
                    "endplay": 38.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "I'm going to bed. I'm very sleepy and it's late.",
                    "frenchspeech": "",
                    "beginplay": 38.5,
                    "endplay": 45.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "You're wrong, today is the twenty-fifth and not the twenty-sixth.",
                    "frenchspeech": "",
                    "beginplay": 45.5,
                    "endplay": 54.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Come close to the fire, you're very cold.",
                    "frenchspeech": "",
                    "beginplay": 54.5,
                    "endplay": 59.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "You're right, I'm freezing. I haven't got a coat and it's snowing. (2)",
                    "frenchspeech": "",
                    "beginplay": 59.5,
                    "endplay": 68.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "She's scared of ghosts and I'm afraid of the dark.",
                    "frenchspeech": "",
                    "beginplay": 68.5,
                    "endplay": 75.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "I'm afraid I can't come, I have an important appointment. (3)",
                    "frenchspeech": "",
                    "beginplay": 75.5,
                    "endplay": 81.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Never mind. You can come on Friday instead. (4)",
                    "frenchspeech": "",
                    "beginplay": 81.5,
                    "endplay": 87.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "The months of the year are: January; February; March; April; May; June; ",
                    "frenchspeech": "",
                    "beginplay": 87.5,
                    "endplay": 102.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "July; August; September; October; November; December.",
                    "frenchspeech": "",
                    "beginplay": 102.5,
                    "endplay": 116
                }
            ]
        }
