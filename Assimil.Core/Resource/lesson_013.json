        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Where do you live?",
                    "frenchspeech": "",
                    "beginplay": 2,
                    "endplay": 5
                },
                {
                    "alterSpeech": true,
                    "speech": "I live in London.",
                    "frenchspeech": "",
                    "beginplay": 5.5,
                    "endplay": 8
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Do you like it?",
                    "frenchspeech": "",
                    "beginplay": 8.5,
                    "endplay": 11
                },
                  {
                    "alterSpeech": true,
                    "speech": "Yes, I like big cities. Do you?",
                    "frenchspeech": "",
                    "beginplay": 12,
                    "endplay": 17.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Not really. I prefer the country.",
                    "frenchspeech": "",
                    "beginplay": 17.5,
                    "endplay": 22
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "How do you spend your evenings in the country?",
                    "frenchspeech": "",
                    "beginplay": 22.5,
                    "endplay": 27
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "I read, I work in the garden. My wife paints.",
                    "frenchspeech": "",
                    "beginplay": 27.5,
                    "endplay": 35.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Does she paint portraits?",
                    "frenchspeech": "",
                    "beginplay": 35.5,
                    "endplay": 38.3
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "No, she paints the bathroom and the hall.",
                    "frenchspeech": "",
                    "beginplay": 38.5,
                    "endplay": 44
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Does your wife like the country too?",
                    "frenchspeech": "",
                    "beginplay": 44.5,
                    "endplay": 48.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "No, she prefers hotels in London.",
                    "frenchspeech": "",
                    "beginplay": 48.6,
                    "endplay": 55
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "To read is the infinitive of the verb. (2)",
                    "frenchspeech": "",
                    "beginplay": 56,
                    "endplay": 62
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "She likes to read novels.",
                    "frenchspeech": "",
                    "beginplay": 62.5,
                    "endplay": 65.6
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "I prefer to live in the country.",
                    "frenchspeech": "",
                    "beginplay": 65.7,
                    "endplay": 69
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "She doesn't like to live in the country.",
                    "frenchspeech": "",
                    "beginplay": 69.5,
                    "endplay": 74
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "There is a lot of work. (3)",
                    "frenchspeech": "",
                    "beginplay": 74.5,
                    "endplay": 79
                }
            ]
        }
