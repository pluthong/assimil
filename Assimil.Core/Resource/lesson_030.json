    "english_lesson": [
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Hello, Joan, is David in?",
                    "frenchspeech": "",
                    "beginplay": 2.5,
                    "endplay": 6.6
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "No I'm afraid not. He isn't home yet.",
                    "frenchspeech": "",
                    "beginplay": 6.6,
                    "endplay": 12.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Is he still working?",
                    "frenchspeech": "",
                    "beginplay": 12.5,
                    "endplay": 14.7
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Yes; Sometimes he works until nine.",
                    "frenchspeech": "",
                    "beginplay": 14.7,
                    "endplay": 19.3
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Oh well, I can't wait.",
                    "frenchspeech": "",
                    "beginplay": 19.3,
                    "endplay": 21.8
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Tell him there's a darts match at the pub tonight. (1)",
                    "frenchspeech": "",
                    "beginplay": 21.8,
                    "endplay": 25.8
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "What time?",
                    "frenchspeech": "",
                    "beginplay": 25.8,
                    "endplay": 26.9
                },
                {
                    "alterSpeech": true,
                    "speech": "About half-past eight.",
                    "frenchspeech": "",
                    "beginplay": 26.9,
                    "endplay": 29.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "If he's back in time, I'll tell him. (2) (3)",
                    "frenchspeech": "",
                    "beginplay": 29.5,
                    "endplay": 32.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Thanks. Bye Joan.",
                    "frenchspeech": "",
                    "beginplay": 32.5,
                    "endplay": 35.5
                },
                {
                    "alterSpeech": true,
                    "speech": "Good-bye Pete.",
                    "frenchspeech": "",
                    "beginplay": 35.5,
                    "endplay": 37.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Hurry up! I'm not ready yet.",
                    "frenchspeech": "",
                    "beginplay": 37.5,
                    "endplay": 45.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Are you still waiting to marry a millionaire?",
                    "frenchspeech": "",
                    "beginplay": 47.5,
                    "endplay": 53
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "He always asks for money and I never have any.",
                    "frenchspeech": "",
                    "beginplay": 53,
                    "endplay": 59.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "I can still remember a few words of German. (4)",
                    "frenchspeech": "",
                    "beginplay": 59.5,
                    "endplay": 65.5
                }
            ]
        },
        {
            "paragraph": [
                {
                    "alterSpeech": false,
                    "speech": "Tell him about the match when he comes in. (5)",
                    "frenchspeech": "",
                    "beginplay": 65.5,
                    "endplay": 70
                }
            ]
        }
